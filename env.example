# Zoom S2S OAuth Credentials
ZOOM_ACCOUNT_ID=your_account_id_here
ZOOM_CLIENT_ID=your_client_id_here
ZOOM_CLIENT_SECRET=your_client_secret_here

# Date range (optional, defaults to last 30 days if not specified)
ZOOM_FROM=2024-01-01
ZOOM_TO=2024-12-31

# User filter (optional, comma-separated emails or user IDs)
ZOOM_USER_FILTER=user1@company.com,user2@company.com

# Output directory (defaults to ./zoom_recordings)
ZOOM_OUTDIR=./zoom_recordings

# Download settings
MAX_CONCURRENT_DOWNLOADS=2
DOWNLOAD_TIMEOUT=300
CHUNK_SIZE=8388608
